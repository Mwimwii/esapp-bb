{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "tsc:watch": "tsc-watch --onSuccess 'node ./dist/index.js'",
    "db:migration:create": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate -n $1",
    "build": "tsc"
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "remove": "^0.1.5",
    "typeorm": "^0.2.37",
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^16.7.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.11.0"
  }
}
